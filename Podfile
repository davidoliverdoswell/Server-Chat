# Uncomment the next line to define a global platform for your project
platform :ios, '11.2'

target 'serverchat' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for serverchat

pod 'Firebase/Core'
pod 'Firebase/Database'
pod 'Firebase/Auth'
pod 'Firebase/Storage'

  target 'serverchatTests' do
    inherit! :search_paths
    # Pods for testing

pod 'Firebase/Core'
pod 'Firebase/Database'
pod 'Firebase/Auth'
pod 'Firebase/Storage'
  end

  target 'serverchatUITests' do
    inherit! :search_paths
    # Pods for testing

pod 'Firebase/Core'
pod 'Firebase/Database'
pod 'Firebase/Auth'
pod 'Firebase/Storage'
  end

end
